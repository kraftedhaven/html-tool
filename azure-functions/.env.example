# Azure Key Vault Configuration
AZURE_KEY_VAULT_URL=https://your-keyvault.vault.azure.net/

# eBay Configuration (stored in Key Vault in production)
EBAY_ENV=production
EBAY_CLIENT_ID=your_ebay_client_id
EBAY_CLIENT_SECRET=your_ebay_client_secret
EBAY_REFRESH_TOKEN=your_ebay_refresh_token
EBAY_FULFILLMENT_POLICY_ID=your_fulfillment_policy_id
EBAY_PAYMENT_POLICY_ID=your_payment_policy_id
EBAY_RETURN_POLICY_ID=your_return_policy_id

# OpenAI Configuration (stored in Key Vault in production)
OPENAI_API_KEY=your_openai_api_key

# Stripe Configuration (stored in Key Vault in production)
